   64 00000000          ;   XllpPmValidateResumeFromSleep  FUNCTION 
   65 00000000          ;  
   66 00000000          ; Parameters: 
   67 00000000          ;   R0: value of PSPR (Power Manager Scratch Pad Register) at reset. 
   68 00000000          ;       - Assumed to be physical address of saved data area 
   69 00000000          ;   R1: contains a condensed copy of the RCSR and PSSR.  
   70 00000000          ;       (RCSR (bits 15:0 in 15:0 of R1) + PSSR (bits 15:0 in 31:16 of R1) 
   71 00000000          ; 
   72 00000000          ; Return value: in R0. 
   73 00000000          ;   0x00: Success.  Sleep and valid checksum in data area 
   74 00000000          ;   0x01: Not a sleep reset. 
   75 00000000          ;   0x02: Other reset in addition to sleep reset.  (This may eventually become a success case.) 
   76 00000000          ;   0x03: Bad checksum for saved data area. 
   77 00000000          ;  
   78 00000000          ; Assumes no stack, MMU disabled. 
   79 00000000          ;       Already initialized: 
   80 00000000          ;           GPIOs, memory controller 
   81 00000000          ; 
   82 00000000          ; Uses (destroys) R0,r1,r2,r3,r4,r5,r6 
   83 00000000          ; 
   84 00000000            
   85 00000000          XllpPmValidateResumeFromSleep  FUNCTION 
   86 00000000            
   87 00000000 e3110004     tst r1, #xlli_RCSR_SMR 
   88 00000004 03a00001     moveq r0, #01 ; Not sleep, return with code. 
   89 00000008 0a000010     beq   XllpPmValidateResumeFromSleep_Lab1    ; lr not corrupted. 
   90 0000000c              ; Sleep detected.  Validate checksum. 
   91 0000000c            
   92 0000000c              ; Save parameters for future use if successful 
   93 0000000c e1a04000     mov r4, r0      ; PSPR, containing phys addr of save area 
   94 00000010 e1a05001     mov r5, r1      ; condensed copy of the RCSR and PSSR. 
   95 00000014 e1a0600e     mov r6, lr 
   96 00000018            
   97 00000018 e2800004     add r0, r0, #SleepState_WordCount ; WordCount is first checksummable location 
   98 0000001c e5901000     ldr r1, [r0]                     ; Get actual wordcount to checksum 
   99 00000020                                               ;  r0..r3 lost in checksum routine 
  100 00000020 e3a02c02     ldr r2, =XLLI_MAX_SLEEP_DATA_COUNT ; Sanity check 
  101 00000024 e1510002     cmp r1, r2 
  102 00000028 c3a00003     movgt r0, #03 ; bad data size, so declare bad checksum. 
  103 0000002c ca000007         bgt       XllpPmValidateResumeFromSleep_Lab1    ; lr not corrupted 
  104 00000030            
  105 00000030 eb00001e     bl XllpPmChecksumSleepDataPh     ; Actual checksum in R0 
  106 00000034 e5942000     ldr r2, [r4, #SleepState_CHKSUM] ; Pre-sleep checksum 
  107 00000038            
  108 00000038 e1a01000     mov     r1, r0 
  109 0000003c e1520000     cmp     r2, r0  
  110 00000040 e3a00000     mov     r0, #0                     ; Assume success 
  111 00000044 13a00003     movne   r0, #03                    ; Bad checksum code. 
  112 00000048            
  113 00000048 e1a01005     mov     r1, r5      ; Restore value.  Will be needed later 
  114 0000004c            
  115 0000004c              ; Need to restore link register 
  116 0000004c e1a0e006     mov lr, r6 
  117 00000050            
  118 00000050          XllpPmValidateResumeFromSleep_Lab1 
  119 00000050            ; lr ready for return 
  120 00000050            
  121 00000050              IF :DEF: Interworking 
  122 00000050                  IF Interworking :LOR: Thumbing 
  123 00000050 e12fff1e          bx  lr 
  124 00000054                  ELSE 
  126 00000054                  ENDIF ; ELSE of IF Interworking :LOR: Thumbing 
  127 00000054              ELSE ; IF :DEF: Interworking 
  129 00000054              ENDIF ; ELSE OF IF :DEF: Interworking 
